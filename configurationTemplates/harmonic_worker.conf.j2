[program:harmonic_worker]
user=root								; The User is set to root, in order to access privialged ports, see issue https://github.com/HASTE-project/HarmonicIO/issues/9
directory=/home/ubuntu/HarmonicIO					; The absolute path is used as dictated by best practices
command=python3.5 -m harmonicIO.worker
autostart=true
autorestart=true
startretires=3
stdout_logfile=/home/ubuntu/HarmonicIO/logs/harmonic_worker.log
stderr_logfile=/home/ubuntu/HarmonicIO/logs/harmonic_worker.err
